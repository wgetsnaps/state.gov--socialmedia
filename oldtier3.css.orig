/* tier 3 page columns */
#body-row01 {width:930px;height:auto;margin:10px 0px 0px 10px;padding:10px 0px 10px 0px;background-color:#fff;}
#body-row01 h3 {margin:0px 0px 0px 10px;display:inline;color:#333;}
#body-row01, #body-row02-col01, #body-row02-col02, #body-row02-col03, #body-row02-col02andcol03, #body-row02-col01andcol02andcol03, #body-row02-col01andcol02 {font-size:11px;font-family:verdana, arial, sans-serif;color:#000;line-height:18px;text-align:left;}

/* breadcrumbs */
.breadcrumbs a {color:#666;}
.underline-11-black {font-size:11px;font-family:verdana, arial, sans-serif;font-weight:normal;color:#000000;text-decoration:underline;}
.underline-11-drkgray {font-size:11px;font-family:verdana, arial, sans-serif;font-weight:normal;color:#333;text-decoration:underline;}
/*#breadcrumbs li:before {content:"> ";}*/
#breadcrumbs li:first-child:before {content:"";}
/* ie workaround for all ie browsers 
* html #breadcrumbs li {background-image:url("/images/2009/gt.gif");background-repeat:no-repeat;background-position:0 50%;padding:0 0.5em 0 1em;margin:0 1em 0 -1em;}
/* win ie browsers - hide from mac ie\*/
* html #breadcrumbs {height:1%;}
* html #breadcrumbs li {display:block;float:left;}
/* end hide */
/* mac ie 5 */
